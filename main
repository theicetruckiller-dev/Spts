local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- STATE VARS
local running = false
local infiniteJump = false
local autoPool = false
local hasTeleported = false
local lastPos = nil
local clickPosition = Vector2.new(401, 270)
local itemsSelected = {}
local selectedPool = 1
local momentorespawn = false
local autoStrength = false
local selectedStrengthStone = 1
local hasTeleStrengthTeleported = false

-- GUI ROOT
local gui = Instance.new("ScreenGui")
gui.Name = "SPTSAuto"
gui.ResetOnSpawn = false
gui.Parent = PlayerGui

-- PANEL
local panel = Instance.new("Frame", gui)
panel.Size = UDim2.new(0, 400, 0, 300)
panel.Position = UDim2.new(0.3, 0, 0.3, 0)
panel.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
panel.Active = true
panel.Draggable = true
Instance.new("UICorner", panel).CornerRadius = UDim.new(0, 12)

-- TITLE
local title = Instance.new("TextLabel", panel)
title.Text = "âš¡ SPTS Auto (By Zwac)"
title.Size = UDim2.new(1, 0, 0, 28)
title.Position = UDim2.new(0, 0, 0, 2)
title.BackgroundTransparency = 1
title.TextColor3 = Color3.fromRGB(230, 230, 230)
title.Font = Enum.Font.GothamBold
title.TextSize = 18

-- CATEGORY BUTTONS
local categories = {"Teleports","Farms","Auto","Misc","Settings"}
local catFrames = {}
for i,cat in ipairs(categories) do
    local btn = Instance.new("TextButton", panel)
    btn.Size = UDim2.new(0, 80, 0, 28)
    btn.Position = UDim2.new(0, 10, 0, 40 + (i-1)*32)
    btn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    btn.Text = cat
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)

    local frame = Instance.new("Frame", panel)
    frame.Size = UDim2.new(1, -100, 1, -50)
    frame.Position = UDim2.new(0, 100, 0, 40)
    frame.BackgroundTransparency = 1
    frame.Visible = i == 1
    catFrames[cat] = frame

    btn.MouseButton1Click:Connect(function()
        for _,f in pairs(catFrames) do f.Visible = false end
        frame.Visible = true
    end)
end

-- AUTO CATEGORY
local autoFrame = catFrames["Auto"]

-- Clicker Button
local clickerBtn = Instance.new("TextButton", autoFrame)
clickerBtn.Size = UDim2.new(0, 120, 0, 32)
clickerBtn.Position = UDim2.new(0, 10, 0, 10)
clickerBtn.BackgroundColor3 = Color3.fromRGB(40,180,100)
clickerBtn.Text = "Clicker: Off"
clickerBtn.TextColor3 = Color3.fromRGB(240,240,240)
clickerBtn.Font = Enum.Font.GothamBold
clickerBtn.TextSize = 14
Instance.new("UICorner", clickerBtn).CornerRadius = UDim.new(0,8)

clickerBtn.MouseButton1Click:Connect(function()
    running = not running
    clickerBtn.Text = running and "Clicker: On" or "Clicker: Off"
    clickerBtn.BackgroundColor3 = running and Color3.fromRGB(200,80,80) or Color3.fromRGB(40,180,100)
end)

-- FARMS FRAME
local farmsFrame = catFrames["Farms"]

-- Strength Stones KORDINATLARI
local strengthStones = {
    Vector3.new(403,249,988), -- normal stone
    Vector3.new(-2275,1943,1051) -- blue crystal
}

-- Strength Stone Selection Buttons
local strengthBtns = {}
for i=1,#strengthStones do
    local btn = Instance.new("TextButton", farmsFrame)
    btn.Size = UDim2.new(0,50,0,26)
    btn.Position = UDim2.new(0,10+(i-1)*60,0,10)
    btn.Text = "Stone "..i
    btn.BackgroundColor3 = (i==1) and Color3.fromRGB(70,70,150) or Color3.fromRGB(70,70,70)
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0,6)
    strengthBtns[i] = btn
    btn.MouseButton1Click:Connect(function()
        selectedStrengthStone = i
        for j,b in ipairs(strengthBtns) do
            b.BackgroundColor3 = (j==i) and Color3.fromRGB(70,70,150) or Color3.fromRGB(70,70,70)
        end
    end)
end

-- Auto Strength Toggle
local autoStrengthBtn = Instance.new("TextButton", farmsFrame)
autoStrengthBtn.Size = UDim2.new(0,120,0,28)
autoStrengthBtn.Position = UDim2.new(0,10,0,50)
autoStrengthBtn.Text = "Auto Strength: Off"
autoStrengthBtn.BackgroundColor3 = Color3.fromRGB(200,120,50)
autoStrengthBtn.TextColor3 = Color3.fromRGB(240,240,240)
autoStrengthBtn.Font = Enum.Font.GothamBold
autoStrengthBtn.TextSize = 14
Instance.new("UICorner", autoStrengthBtn).CornerRadius = UDim.new(0,8)

autoStrengthBtn.MouseButton1Click:Connect(function()
    autoStrength = not autoStrength
    autoStrengthBtn.Text = autoStrength and "Auto Strength: On" or "Auto Strength: Off"
    hasTeleStrengthTeleported = false
end)

-- CLICK LOOP
task.spawn(function()
    while true do
        if running and clickPosition then
            VirtualInputManager:SendMouseButtonEvent(
                clickPosition.X, clickPosition.Y,0,true,game,1
            )
            VirtualInputManager:SendMouseButtonEvent(
                clickPosition.X, clickPosition.Y,0,false,game,1
            )
        end
        task.wait(0.05)
    end
end)

-- AUTO POOL LOOP
local pools = {
    Vector3.new(366,250,-446),
    Vector3.new(348,264,-499),
    Vector3.new(1639,260,2250),
    Vector3.new(-2303,977,1072),
    Vector3.new(-2051,714,-1893)
}
local teleFrame = catFrames["Teleports"]
local poolBtns = {}

for i=1,#pools do
    local btn = Instance.new("TextButton", teleFrame)
    btn.Size = UDim2.new(0,50,0,26)
    btn.Position = UDim2.new(0,10+(i-1)*60,0,10)
    btn.Text = "Pool "..i
    btn.BackgroundColor3 = Color3.fromRGB(70,70,70)
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0,6)
    poolBtns[i] = btn
    btn.MouseButton1Click:Connect(function()
        selectedPool = i
        for j,b in ipairs(poolBtns) do
            b.BackgroundColor3 = (j==i) and Color3.fromRGB(70,70,150) or Color3.fromRGB(70,70,70)
        end
    end)
end

local autoPoolBtn = Instance.new("TextButton", teleFrame)
autoPoolBtn.Size = UDim2.new(0,120,0,28)
autoPoolBtn.Position = UDim2.new(0,10,0,50)
autoPoolBtn.Text = "Auto Pool: Off"
autoPoolBtn.BackgroundColor3 = Color3.fromRGB(200,120,50)
autoPoolBtn.TextColor3 = Color3.fromRGB(240,240,240)
autoPoolBtn.Font = Enum.Font.GothamBold
autoPoolBtn.TextSize = 14
Instance.new("UICorner", autoPoolBtn).CornerRadius = UDim.new(0,8)
autoPoolBtn.MouseButton1Click:Connect(function()
    autoPool = not autoPool
    autoPoolBtn.Text = autoPool and "Auto Pool: On" or "Auto Pool: Off"
    hasTeleported = false
end)

-- Teleport Functions
local function doTeleport()
    local char = Player.Character
    if char and char:FindFirstChild("HumanoidRootPart") and autoPool then
        char.HumanoidRootPart.CFrame = CFrame.new(pools[selectedPool])
        hasTeleported = true
    end
end

local function doStrengthTeleport()
    local char = Player.Character
    if char and char:FindFirstChild("HumanoidRootPart") and autoStrength then
        char.HumanoidRootPart.CFrame = CFrame.new(strengthStones[selectedStrengthStone])
        hasTeleStrengthTeleported = true
    end
end

RunService.RenderStepped:Connect(function()
    local char = Player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        -- Pool reset
        local pos = char.HumanoidRootPart.Position
        if lastPos and (pos-lastPos).Magnitude > 1 then hasTeleported = false end
        lastPos = pos
        -- Strength reset
        if lastPos and (pos-lastPos).Magnitude > 1 then hasTeleStrengthTeleported = false end
    end
end)

-- AUTO POOL + AUTO STRENGTH LOOP
task.spawn(function()
    while true do
        if autoPool and not hasTeleported then doTeleport() end
        if autoStrength and not hasTeleStrengthTeleported then doStrengthTeleport() end

        -- SPAWN BYPASS
        local char = Player.Character
        if char and char:FindFirstChild("Humanoid") then
            local hum = char.Humanoid
            local c = math.floor(hum.Health)
            local d = math.floor(hum.MaxHealth)*60/100
            if c<=d then momentorespawn=true end
            if momentorespawn then
                char:Remove()
                task.wait(0.1)
                ReplicatedStorage.RemoteEvent:FireServer({"Respawn"})
                task.wait(0.1)
                momentorespawn=false
            end
        end

        -- AUTO STRENGTH CLICK
        if autoStrength then
            ReplicatedStorage.RemoteEvent:FireServer({{"Add_FS_Request"}})
        end

        task.wait(0.4)
    end
end)

-- DEATH / RESPAWN HANDLER
local function setupDeathHandler()
    local function connectDied()
        local char = Player.Character
        if char then
            local hum = char:FindFirstChildOfClass("Humanoid")
            if hum then
                hum.Died:Connect(function()
                    hasTeleported=false
                    hasTeleStrengthTeleported=false
                    task.wait(1)
                    Player.CharacterAdded:Wait()
                    task.wait(0.5)
                    doTeleport()
                    doStrengthTeleport()
                end)
            end
        end
    end
    Player.CharacterAdded:Connect(function() task.wait(0.5); connectDied() end)
    connectDied()
end
setupDeathHandler()
